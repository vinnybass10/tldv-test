{
  "name": "Tldv",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "npx ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/infra/server.ts",
    "run-migrations": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "revert-migrations": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "start": "node dist/app.js",
    "test": "jest --no-cache",
    "typeorm": "npx ts-node-dev ./node_modules/typeorm/cli"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.0.6",
    "prettier": "^2.5.1",
    "ts-jest": "^27.0.4",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/jsonwebtoken": "^8.5.8",
    "celebrate": "^15.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41"
  }
}
